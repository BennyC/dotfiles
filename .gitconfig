# This is Git's per-user configuration file.
[user]
	name = Ben Colegate
	email = ben.colegate@fourthwallcreative.com
[alias]
	s = status -s
  l = log --abbrev-commit
	switch = !/usr/local/bin/legit switch
	sync = !/usr/local/bin/legit sync
	publish = !/usr/local/bin/legit publish
	unpublish = !/usr/local/bin/legit unpublish
	undo = !/usr/local/bin/legit undo
	branches = !/usr/local/bin/legit branches
[core]
	excludesfile = /Users/bencolegate/.gitignore
	autocrlf = input
